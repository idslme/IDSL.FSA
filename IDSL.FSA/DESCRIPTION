Package: IDSL.FSA
Type: Package
Title: Fragmentation Spectra Analysis (FSA)
Version: 1.0
Authors@R: c(
    person(family = "Fakouri-Baygi", given = "Sadjad", email = "sadjad.fakouri-baygi@mssm.edu",
    role = c("cre","aut"), comment = c(ORCID = "0000-0002-6864-6911")),
